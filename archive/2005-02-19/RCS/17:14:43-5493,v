@sub bang {
    my $self = shift;
    my $pan = 1;
    if ($self->{bangs} % 3 == 0) {
        $pan = 0;
    }
    elsif ($self->{bangs} % 3 == 1) {
        $pan = -1
    }
    
    if ($self->{bangs} % 32 < 3) {
        $self->t({pan => $pan, speed => 0.05, volume => 0.5});
    }
}@
