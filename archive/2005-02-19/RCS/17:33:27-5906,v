@sub bang {
    my $self = shift;
    if ($self->{bangs} % 4 == 0) {
        $self->t({sample => "drumtraks/6", accellerate => 0.001,
                  speed => 1,
                  volume => 0.06,
                  shape => 0.4
                 }
                );
    }
}@
