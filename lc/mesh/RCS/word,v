my $move = {
    'across'      => {x => -8,  y => 0,    speed => 0.00001,  angle => pi()/2},
    # not used
    'diagonal'    => {x => -8,  y => -3,   speed => 0.000011, angle => pi()/2.6},
    'in'          => {x => -8,  y => 0,    speed => 0.000005, angle => pi()/2},
    'out'         => {x => 0,   y => 0,    speed => 0.000005, angle => pi()/2},
    'static_edge' => {x => 0,   y => 3.99, speed => 0,        angle => 0},
    'static_middle' => {x => 0, y=> 0,     speed => 0,        angle => 0}
};

my $strikes = {'average',    => {mass => 1,   alpha => 1},
	       'heavy stiff' => {mass => 14,  alpha => 0.2},
	       'heavy soft'  => {mass => 3,   alpha => 2},
	       'light stiff' => {mass => 0.5, alpha => 0.1},
	       'light soft'  => {mass => 0.5, alpha => 3},
	      };

my $skins = {'average'         => {tension => 1,   loss => 1},
             'tense dry'    => {tension => 1.1, loss => 0.999},
             'loose dry'    => {tension => 0.9, loss => 0.999},
             'tense wet' => {tension => 1.1, loss => 1.03},
             'loose wet' => {tension => 0.9, loss => 1.03}
};

my $mallet = {across =>
	      {heavy =>
	       {stiff => 'q',
		soft  => 'r'
	       },
	       light => 
	       {stiff => 'y',
		soft  => 's'
	       },
	      },
	      in =>
	      {heavy =>
	       {stiff => 'c',
		soft  => 'm'
	       },
	       light => 
	       {stiff => 'f',
		soft  => 'w'
	       },
	      },
	      out =>
	      {heavy =>
	       {stiff => 'k',
		soft  => 'n'
	       },
	       light => 
	       {stiff => 'v',
		soft  => 'z'
	       },
	      },
	      static_edge =>
	      {heavy =>
	       {stiff => 'x',
		soft  => 'd'
	       },
	       light => 
	       {stiff => 't',
		soft  => 'b'
	       },
	      },
	      static_middle =>
	      {heavy =>
	       {stiff => 'j',
		soft  => 'g'
	       },
	       light => 
	       {stiff => 'p',
		soft  => 'h'
	       },
	      }
	     };

my $mesh = {average => 'a',
	    'tense' => {dry => 'e',
			wet => 'i'
		       },
	    'loose' => {try => 'o',
			wet => 'u'
		       }

	   };


sub invert {
  my ($hash, @@p) = @@_;
  my @@result;
  if (ref($hash)) {
      foreach my $key (keys %$hash) {
	  @@result = (@@result, invert($hash->{$key}, @@p, $key));
      }
  }
  else {
      @@result = ($hash, \@@p);
  }
  return(@@result);
}

{
    my ($consonants, $vowels);

    sub lookup {
	my ($letter) = @@_;
	my $result;
	$consonants ||= {invert($mallet)};
	$vowels     ||= {invert($mesh)};
	
	if (exists $vowels->{$letter}) {
	  my $key = join(' ', @@{$vowels->{$letter}});
	  $result = $skins->{$key};
	    
	}
	else {
	    my $keys = $consonants->{$letter};
	    $result = {%{$move->{shift @@$keys}},
		       %{$strikes->{join(' ', @@$keys)}}
		      };
	}
	return($result);
    }
@


