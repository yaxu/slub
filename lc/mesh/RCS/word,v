sub play {
    my $self = shift;
    my $word = shift;
    while ($word =~ /([bcdfghjklmnpqrstvwxyz]*)([aeiou])/g) {
        my $cs = $1;
        my $v = $2;
        #$cs = 'w' if !$cs;
        my $h = Word::lookup($v);
        $self->tension($h->{tension});
        $self->loss($h->{loss});
        foreach my $c (split(//, $cs)) {
            $h = Word::lookup($c);
            $h->{velocity} = 0.9;
            $self->vector($h);
            sleep(0.02 + rand(0.02));
        }
@


